<include>
    <context name="hardline">
        <extension name="blocked_vanilla">
            <condition field="destination_number" expression="^.*$">
                <action application="log" data="WARNING Hardline: vanilla dialplan hit (backend/xml_curl missing)"/>
                <action application="hangup" data="NORMAL_TEMPORARY_FAILURE"/>
            </condition>
        </extension>
    </context>
</include>
